{
  "name": "johanbrook.com",
  "private": true,
  "version": "0.0.1",
  "description": "Homepage of Johan Brook.",
  "main": "build.js",
  "config": {
    "port": 8000
  },
  "scripts": {
    "build": "NODE_ENV=production node --harmony -e 'require(\"./build\")()'",
    "test": "./node_modules/eslint/bin/eslint.js .",
    "serve": "NODE_ENV=development node --harmony server.js",
    "deploy": "npm run build && ./deploy",
    "new": "coffee scripts/new.coffee --silent '${TITLE}' | xargs $EDITOR"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/johanbrook/johanbrook.com"
  },
  "author": "Johan Brook <johan@johanbrook.com>",
  "license": "Copyright Johan Brook (2016)",
  "bugs": {
    "url": "https://github.com/johanbrook/johanbrook.com/issues"
  },
  "homepage": "https://github.com/johanbrook/johanbrook.com",
  "dependencies": {
    "handlebars": "^3.0.1",
    "lodash": "^3.8.0",
    "metalsmith": "^1.6.0",
    "metalsmith-autoprefixer": "^1.0.2",
    "metalsmith-branch": "0.0.4",
    "metalsmith-collections": "^0.7.0",
    "metalsmith-excerpts": "^1.0.0",
    "metalsmith-feed": "0.0.6",
    "metalsmith-gzip": "^1.1.0",
    "metalsmith-layouts": "^1.0.0",
    "metalsmith-markdown": "^0.2.1",
    "metalsmith-metallic": "^0.3.1",
    "metalsmith-pagination": "^1.1.0",
    "metalsmith-permalinks": "^0.4.0",
    "metalsmith-redirect": "^1.0.2",
    "metalsmith-sass": "^1.3.0",
    "metalsmith-templates": "^0.7.0",
    "metalsmith-typography": "^0.1.1",
    "metalsmith-word-count": "0.0.4",
    "moment": "^2.10.2",
    "slug": "^0.9.1"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.6",
    "colors": "^1.0.3",
    "front-matter": "~0.2.0",
    "metalsmith-watch": "github:johanbrook/metalsmith-simplewatch",
    "q": "~1.0.1",
    "q-io": "~1.11.0",
    "slug": "~0.4.2",
    "underscore.string": "^3.0.3",
    "yamljs": "~0.1.4"
  }
}
